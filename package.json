{
  "name": "language-kevscript",
  "version": "0.3.6",
  "description": "KevScript language support in Atom",
  "main": "lib/main.js",
  "keywords": [
    "kevoree",
    "grammar",
    "kevscript",
    "script",
    "syntax",
    "highlighter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kevoree/atom-kevscript-language.git"
  },
  "license": "LGPL-3.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutoComplete"
      }
    },
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^4.5.0",
    "kevoree-const": "^1.0.1",
    "kevoree-kevscript": "^4.1.3",
    "tiny-conf": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "globals": {
      "atom": true,
      "Promise": true
    }
  }
}
